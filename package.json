{
  "name": "scalable-node",
  "version": "0.1.0",
  "description": "A scalable nodejs application.",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --runInBand",
    "prepare": "husky install",
    "release": "standard-version",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -f ./ormconfig.local.ts",
    "migration:generate": "yarn run typeorm migration:generate -n",
    "migration:blank": "yarn run typeorm migration:create -n",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "argon2": "^0.28.2",
    "connect-redis": "^6.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "graphql": "^15.5.1",
    "joi": "^17.4.2",
    "joi-password-complexity": "^5.1.0",
    "mysql2": "^2.3.0",
    "nodemailer": "^6.6.3",
    "redis": "^3.1.2",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.37",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/connect-redis": "^0.0.17",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^2.8.31",
    "@types/supertest": "^2.0.11",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typed-emitter": "^1.3.1",
    "typescript": "^4.3.5"
  },
  "keywords": [],
  "author": {
    "name": "Jayant Malik",
    "url": "https://developbharat.com",
    "email": "prod.jayantmalik@gmail.com"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/developbharat/scalable-node.git"
  },
  "bugs": {
    "email": "prod.jayantmalik@gmail.com",
    "url": "https://github.com/developbharat/scalable-node/issues"
  },
  "license": "ISC"
}
